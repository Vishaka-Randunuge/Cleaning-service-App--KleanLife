<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.kleanlife">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PropertyCleaners"
        tools:targetApi="31">
        <activity
            android:name="com.example.kleanlife.ViewReviewActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.GiveReviewActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.ReviewActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.PublishDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".Adapter"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.ViewUserPublishActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.Published"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.ViewPropertyDetailsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.PropertyDetailsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.ForgotPassActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.KleanActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.example.kleanlife.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>